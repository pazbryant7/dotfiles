Xft.rgba: rgb
Xft.hinting: 1
Xft.antialias: 1
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! Global colors
*.background: #1d1f21
*.foreground: #ffffff
*.cursorColor: #957139

! ST specific configurations
st.font: Maple Mono:pixelsize=13.5:antialias=true:autohint=true:style:Bold
! st.font: Maple Mono NL:pixelsize=13.5:style:SemiBold:antialias=true:autohint=true
! st.font: Maple Mono NL NF:pixelsize=13.5:style:SemiBold:antialias=true:autohint=true
! st.font: Maple Mono Normal NL NF:pixelsize=13.5:style:SemiBold:antialias=true:autohint=true
! st.font: Maple Mono Normal NL:pixelsize=13.5:style:SemiBold:antialias=true:autohint=true
! st.font: Recursive Mono Casual Static:pixelsize=13.5:style:SemiBold:antialias=true:autohint=true


st.alpha: 0.85
st.chscale: 1.00
st.cwscale: 1.00
st.borderpx: 0px

! DWM specific configurations
dwm.selbgcolor: #957139
dwm.selfgcolor: #ffffff
dwm.normbgcolor: #000000
dwm.normfgcolor: #817f7f
dwm.selbordercolor: #957139
